include <iostream>
using namespace std;
int main(int n) {
	int r = 50000000;
	cout << "Enter N - ";
	cin >> n;
	int a[100] = {};
	for (int i = 0; i < n; i++) {
		a[i] = 5 * -(i - 1);
		cout << a[i] << endl;
	}
	for (int j = 2; j < n; j += 2) {
		if (a[j] < r) {
			r = a[j];
		}
	}
	cout << "min element = " << r;
