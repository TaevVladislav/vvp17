#include <iostream>
using namespace std;
int main(int n) {
	int r = 0;
	cout << "Enter N - ";
	cin >> n;
	int a[100] = {};
	for (int i = 0; i < n; i++) {
		a[i] = 5 * -(i - 1);
		cout << a[i] << endl;
	}
	for (int j = 2; j < n; j++) {
		if (j != n - 1) {
			if (a[j - 1] > a[j - 2] or a[j - 1] > a[j]) {
				r = j;
			}
		}
		else {
			if (a[j - 1] > a[j - 2]) {
				r = j;
			}
		}
	}
	if (r == 0) {
		cout << "This element doesn't exist :_(";
	}
	else {
		cout << "Number of last element - " << r + 1;
	}
}
