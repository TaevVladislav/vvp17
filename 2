#include <iostream>
using namespace std;
int main(int n) {
	int r = 0; int c = 0;
	cout << "Enter N - ";
	cin >> n;
	int a[100] = {5};
	cout << a[0] << endl;
	for (int i = 1; i < n; i++) {
		a[i] = a[i - 1] + 18;
		cout << a[i] << endl;
	}
	for (int j = n; j > 1; j--) {
		if (a[j] - a[j - 1] == a[j - 1] - a[j - 2]) {
			c += 1;
		}
	}
	if (c == n - 2) {
		cout << "True, d = " << a[n - 1] - a[n - 2];
	}
	else {
		cout << "False, 0";
	}
}
