#include <iostream>
using namespace std;
int main() {
	int r;
	int a[8] = {9, 4, 1, -9, 0, 2, 9, -11};
	int n = 8;
	for (int j = 0; j < n; j++) {
		int r = a[j];
		for (int i = j + 1; i < n; i++) {
			if (r == a[i]) {
				cout << "Number 1 - " << j + 1<< endl;
				cout << "Number 2 - " << i + 1<< endl;
			}
		}
	}
}
